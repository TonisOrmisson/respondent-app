{
    "name": "surveyapp/backend",
    "description": "Survey App Backend API using Logiscape MCP SDK",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "logiscape/mcp-sdk-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SurveyApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SurveyApp\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8088 public/index.php",
        "test": "phpunit"
    }
}
